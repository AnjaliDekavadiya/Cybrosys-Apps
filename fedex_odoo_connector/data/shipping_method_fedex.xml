<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data noupdate="1">
        <record id="fedex_package_type" model="stock.package.type">
        <!--Creating a new package type-->
            <field name="name">Fedex_box</field>
            <field name="shipper_package_code">Fedex</field>
            <field name="max_weight">30.0</field>
        </record>
        <record id="product_category_deliveries" model="product.category">
            <!--Creating a new product category-->
            <field name="parent_id" ref="product.product_category_all"/>
            <field name="name">Deliveries</field>
        </record>
        <record id="product_product_delivery_fedex"
                model="product.product">
            <!--Creating a new product-fedex-->
            <field name="name">Fedex</field>
            <field name="default_code">Delivery_008</field>
            <field name="type">service</field>
            <field name="categ_id" ref="delivery.product_category_deliveries"/>
            <field name="sale_ok" eval="False"/>
            <field name="purchase_ok" eval="False"/>
            <field name="list_price">0.0</field>
            <field name="invoice_policy">order</field>
        </record>
        <record id="delivery_carrier_fedex" model="delivery.carrier">
            <!--Creating a new delivery carrier-->
            <field name="name">Fedex</field>
            <field name="delivery_type">fedex_api</field>
            <field name="product_id" ref="fedex_odoo_connector.product_product_delivery_fedex"/>
        </record>
    </data>
</odoo>
